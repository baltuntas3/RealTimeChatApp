const app = require("../");
const websocketServer = require("../websocket/websocket");

const port = process.env.PORT || 5000;
// Express
app.listen(port, () => {
    console.log(`Started listening on ${port}`);
});
// Websocket
websocketServer.listen(3005, "0.0.0.0", () => {
    console.log("listening on *:3005");
});

